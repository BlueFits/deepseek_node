{
  "name": "deepseek-custom",
  "version": "0.0.1",
  "description": "A guide to REST API using Typescript created for Toptal blog",
  "bin": {
    "jarv": "dist/bin/index.js"
  },
  "scripts": {
    "start": "tsc && node ./dist/app.js",
    "debug": "export DEBUG=* && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --ext ts --exec ts-node app.ts",
    "build": "tsc",
    "check-bin": "npm run build && node ./dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makinhs/toptal-rest-series.git"
  },
  "keywords": [
    "REST",
    "API",
    "ExpressJS",
    "NodeJS",
    "Toptal"
  ],
  "author": "Marcos Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makinhs/toptal-rest-series/issues"
  },
  "homepage": "https://github.com/makinhs/toptal-rest-series#readme",
  "dependencies": {
    "argon2": "^0.27.0",
    "axios": "^1.7.9",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dockerode": "^2.5.5",
    "elevenlabs": "^1.50.4",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "shortid": "^2.2.16",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/dockerode": "^3.3.34",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.2",
    "@types/shortid": "0.0.29",
    "nodemon": "^3.1.9",
    "source-map-support": "^0.5.16",
    "ts-node": "^10.9.2",
    "tslint": "^6.0.0",
    "typescript": "^5.7.3"
  }
}
